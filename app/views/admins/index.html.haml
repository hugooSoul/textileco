.wrapper_with_padding

  %h2 Admin Section

  %table.table.table-bordered
    %thead
      %tr
        %th ID
        %th Name
        %th Last Name
        %th Email
        %th Action

    %tbody
      - @employees.each do |employee|
        - unless employee.admin?
          %tr
            %td= employee.id
            %td= employee.first_name
            %td= employee.last_name
            %td= employee.email
            %td= link_to "report", "admins/#{employee.id}/report"
